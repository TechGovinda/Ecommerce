{
  "version": 2,
  "builds": [
    {
      "src": "ecommerce/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.11.7" }
    },
    {
      "src": "build_static.sh",
      "use": "@vercel/bash"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "ecommerce/wsgi.py"
    }
  ]
}
